# Define two sets
set1 = {1, 2, 3, 4}
set2 = {3, 4, 5, 6}

# Find intersection
common_elements = set1 & set2  

print("Set 1:", set1)
print("Set 2:", set2)
print("Intersection:", common_elements)


 # Define two sets
set1 = {1, 2, 3, 4}
set2 = {3, 4, 5, 6}

# Find intersection
common_elements = set1 & set2  # or set1.intersection(set2)

print("Set 1:", set1)
print("Set 2:", set2)
print("Intersection:", common_elements)
